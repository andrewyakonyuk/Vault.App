    <DebugType>portable</DebugType>
    <PackageProjectUrl>https://github.com/andrewyakonyuk/Vault.App/</PackageProjectUrl>
    <Authors>Andrew Yakonyuk</Authors>
    <Description>Vault.Spouts.Abstractions</Description>
    <AssemblyName>Vault.Spouts.Abstractions</AssemblyName>
    <PackageId>Vault.Spouts.Abstractions</PackageId>
